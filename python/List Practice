l=[1,2,3,3,4]
print(l)
l=[1,2,3,[34,5,2]]
print(l)
l=[1,2,3,4,5]
print(l[0:3])
l=[1,4,2,[3,4,3]]
print(l[3][1])
l=[[[1,2,3,2],[5,2,9,1]],[[2,3,5,2,4,4],[342,4,2,44,]],[[2,3,5,87,12,34],[83,54,3,4,3]]]
print(l[2][0][2])
l=[1,2,3,4,5,6,7,8]
print(l[0:4])
print(l[1:])
print(l[0::2])
print(l[2:7:2])
print(l[::-1])
print(l[-5::2])
l.append(34)
print(l)
l.extend([9,10,11])
print(l)
l.append([2,3,4])
print(l)
l.extend('hammad')
print(l)
