# /editing in list
l=[12,2,3,5,6,7,32]
l[0]=400
print(l)
l[2:]=8,7,5,4,4,3,5,6,3,1
print(l)
# delettion in list
print(l)
# deletion with indexing
del l[0]
print(l)
# deleltion with slicing
del l[2:4]
print(l)
# remove keywords
l.remove(5)
print(l)
# pop keyword use for delete last item if index not provided
l.pop()
print(l)
